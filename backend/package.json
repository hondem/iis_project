{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.ts",
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0",
    "python": "2.7.17"
  },
  "scripts": {
    "test": "echo \"Error: No tests available\" && exit 0",
    "build": "tsc",
    "start": "ts-node ./src/app.ts",
    "dev": "nodemon --watch './src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' ./src/app.ts",
    "lint": "eslint --ext ts --report-unused-disable-directives ./src",
    "migrate:latest": "ts-node ./node_modules/.bin/knex migrate:latest --cwd ./src/database --knexfile ../config/knexfile.ts",
    "migrate:make": "ts-node ./node_modules/.bin/knex migrate:make --cwd ./src/database --knexfile ../config/knexfile.ts",
    "migrate:rollback": "ts-node ./node_modules/.bin/knex migrate:rollback --all --cwd ./src/database --knexfile ../config/knexfile.ts",
    "migrate:down": "ts-node ./node_modules/.bin/knex migrate:down --cwd ./src/database --knexfile ../config/knexfile.ts",
    "migrate:up": "ts-node ./node_modules/.bin/knex migrate:up --cwd ./src/database --knexfile ../config/knexfile.ts",
    "seed:make": "ts-node ./node_modules/.bin/knex seed:make --cwd ./src/database --knexfile ../config/knexfile.ts",
    "seed:run": "ts-node ./node_modules/.bin/knex seed:run --cwd ./src/database --knexfile ../config/knexfile.ts",
    "docker:build": "docker build -t payday_api ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonschema": "^1.1.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.141",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.0",
    "dotenv": "^8.1.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "knex": "^0.19.4",
    "koa": "^2.8.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "objection": "^1.6.11",
    "pg": "^7.12.1",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@strv/eslint-config-node": "^2.1.2",
    "@strv/eslint-config-typescript": "^2.1.3",
    "@types/bluebird": "^3.5.27",
    "@types/kcors": "^2.2.3",
    "@types/koa": "^2.0.50",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^12.7.9",
    "@types/pino": "^5.8.10",
    "eslint": "~6.7.1",
    "nodemon": "^1.19.3",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
